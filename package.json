    "build:ssr": "ng build --prod && ng run kubiiksweb:server:production",
    "prerender": "ng run kubiiksweb:prerender",
    "build:vps": "ng build --prod --configuration=vps && ng run kubiiksweb:server:production  && node ./vps.js"
  },
